% EN-dash / EM-dash などがあると (u)pbibtex がフリーズする問題
@inbook{inbook,
   author = "Donald E. Knuth",
   title = "Fundamental Algorithms",
   publisher = "Addison-Wesley",
   year = "1973",
   pages = "10--119"
}

% U+2013: en-dash (pbibtex -> ^^-escaped)
@inbook{inbook-2013,
   author = "Donald E. Knuth",
   title = "Fundamental Algorithms",
   publisher = "Addison-Wesley",
   year = "1973",
   pages = "10–119",
}

